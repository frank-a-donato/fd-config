Host *
  ForwardAgent yes
  ServerAliveInterval 5
  GSSAPIAuthentication yes
  PreferredAuthentications gssapi-with-mic,publickey,keyboard-interactive,password
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p

Host bnl.notes.dutc.io beeland
    HostName bnl.notes.dutc.io
#    ControlMaster auto
#    ControlPath ~/.ssh/%r@%h-%p
#    ControlPersist 600
	User	fdonato
	IdentityFile	~/.ssh/beeland

Host bnlmotionstudy.notes.dutc.io bnlmotionstudy
	HostName bnlmotionstudy.notes.dutc.io
	User	fdonato
	IdentityFile	~/.ssh/motionstudy

#
# Gateways for N2SN
#

Host ssh.nsls2.bnl.gov
  ControlMaster yes
  PubKeyAuthentication no
  GSSAPIAuthentication no
  LocalForward 1188 dc1.bnl.gov:88
  LocalForward 1288 dc2.bnl.gov:88
  LocalForward 1388 dc3.bnl.gov:88

#
# Connections to N2SN Systems
#

Host n2sn
  HostName heartofgold.nsls2.bnl.gov
  PubKeyAuthentication no
  GSSAPIDelegateCredentials yes
  ProxyJump ssh.nsls2.bnl.gov
  DynamicForward    9090
